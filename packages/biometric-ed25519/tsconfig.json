{
    "extends": "../../tsconfig.browser.json",
    "compilerOptions": {
        "outDir": "./lib",
    }, 
    "typedocOptions": {
        "skipErrorChecking": true,
        "entryPoints": ["src"],
        "entryPointStrategy": "expand",
        "out": "docs",
        "exclude": ["node_modules"],
        "excludeNotDocumented": true,
        "excludeNotDocumentedKinds": [
            "Module",
            "Reference"
        ],
        "githubPages": false,
    },
    "files": [
        "src/index.ts",
    ]
}
